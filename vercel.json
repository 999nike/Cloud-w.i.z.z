{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/wizz.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/memory.js", "dest": "/public/memory.js" },
    { "src": "/api/(.*)", "dest": "/api/wizz.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


