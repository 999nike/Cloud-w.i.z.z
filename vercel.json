{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/wizz.js" },
    {
      "src": "/CloudWizz_RuleCore_v2.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/CloudWizz_RuleCore_v2.js"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
